<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>EduUp</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
</head>
<body>
    <header>
        <nav class="navbar navbar-light" style="background-color: #e3f2fd;">
            <div class="d-flex w-100 justify-content-between">
                <div class="p-2">
                    <a class="navbar-brand mx-3">Edu Up</a>
                </div>
                <div class="p-2">
                    <button class="btn btn-outline-success mx-2" type="button">Categories</button>
                </div>
                <div class="p-2">
                    <button class="btn btn-outline-success mx-2" type="button">Courses</button>
                </div>
                <div class="p-2">
                    <button class="btn btn-outline-success mx-2" type="button">Authors</button>
                </div>
                 <div class="p-2">
                    <button class="btn btn-outline-success mx-2" type="button">Orders</button>
                </div>
                <div class="p-2">
                    <button class="btn btn-outline-success mx-2" type="button">Users</button>
                </div>
                <div class="p-2 w-100 d-flex justify-content-end">
                    <div>
                        <button class="btn btn-outline-secondary me-3 d-flex justify-content-end" type="button">profile</button>
                    </div>
                </div>
            </div>
        </nav>
    </header>
    <div>
        @RenderBody()
    </div>
    <script src="~/vendor/jquery/jquery.min.js"></script>
    <script src="~/vendor/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/js/isotope.min.js"></script>
    <script src="~/js/owl-carousel.js"></script>
    <script src="~/js/tabs.js"></script>
    <script src="~/js/popup.js"></script>
    <script src="~/js/custom.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
